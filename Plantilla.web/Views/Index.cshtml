<div class="modal fade" id="ModalMixto" name="ModalMixto" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Pago Mixto</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group row">
                        <div class="col-md-6">
                            <label class="control-label">Monto total:</label>
                            <input type="text" class="form-control" name="monTotal" id="monTotal" disabled>
                        </div>
                        <div class="col-md-6">
                            <label class="control-label">Monto Efectivo:</label>
                            <input type="number" class="col-md-12 form-control" name="monEfe" id="monEfe" required disabled>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-md-6">
                            <label>Tipo de Tarjeta:</label>
                            <select class="form-control m-select2" id="monTip" name="monTip">
                                <option value="">Seleccione una tarjeta</option>
                                @if (ViewBag.Tarjetas != null)
                                {
                                    foreach (var item in ViewBag.Tarjetas)
                                    {
                                        <option value="@item.TIPO_TARJETA">@item.TIPO_TARJETA</option>
                                    }
                                }
                            </select>
                        </div>
                        <div class="col-md-6">
                            <label class="control-label">Monto Tarjeta:</label>
                            <input type="text" class="form-control" name="monTar" id="monTar" onkeyup="calcularMonto()" required value="">
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button id="btnCancelar" name="btnCancelar" class="btn btn-info" data-allow-outside-click="true" onclick="GenerarFactura('Mixto',$('select[name=\'monTip\']').val());">Guardar</button>
                <button id="btnCancelar" name="btnCancelar" class="btn btn-warning" data-allow-outside-click="true" data-confirm-button-class="btn-danger" data-dismiss="modal">Cancelar</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="ModalPagoTarjeta" name="ModalPagoTarjeta" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-small">
        <div class="modal-content">
            <div class="modal-header bg-green font-white">
                <h2 class="modal-title font-white">Selección de Tarjeta</h2>
            </div>
            <div class="modal-body">
                <div class="kt-portlet__body">
                    <div class="kt-notification-v2">
                        @if (ViewBag.Tarjetas != null)
                        {
                            foreach (var item in ViewBag.Tarjetas)
                            {
                                <a class="kt-notification-v2__item" onclick="GenerarFactura('T','@item.TIPO_TARJETA');">
                                    <div class="kt-notification-v2__item-icon">
                                        <img src='@Url.Content("~/assets/Img/Sistema/Tarjeta/" + (item.TIPO_TARJETA == "ND" ? "americanexpress" : item.TIPO_TARJETA) + ".png")' style="width:40px;" alt="logo" class="logo-default">
                                    </div>
                                    <div class="kt-notification-v2__itek-wrapper">
                                        <div class="kt-notification-v2__item-title">
                                            @item.TIPO_TARJETA
                                        </div>
                                        <div class="kt-notification-v2__item-desc">
                                            Tarjeta
                                        </div>
                                    </div>
                                </a>
                            }
                        }

                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="btnCancelar" name="btnCancelar" class="btn btn-danger" data-allow-outside-click="true" data-confirm-button-class="btn-danger" data-dismiss="modal">Cancelar</button>

            </div>
        </div>
    </div>
</div>
