                            <div class="kt-section__content kt-section__content--solid">
                                <button type="button" name="mixto" id="mixto" hover="btn-outline-hover-primary" normal="btn-outline-primary" class="btn btncito btn-outline-hover-primary" onclick="MostrarModalMixto();">Mixto</button>
                                <button type="button" name="efectivo" id="efectivo" hover="btn-outline-hover-brand" normal="btn-outline-brand" class="btn btncito btn-outline-hover-brand" onclick="GenerarFactura('E','ND')">Efectivo</button>
                                <button type="button" name="tarjeta" id="tarjeta" hover="btn-outline-hover-success" normal="btn-outline-success" class="btn btncito btn-outline-hover-success" onclick="MostrarModalTarjeta();">Tarjeta</button>
