
@{
    ViewBag.Title = "Transferencias bancarias";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script type='text/javascript' src='http://paperjs.org/assets/js/paper.js'></script>
<!--begin::Content wrapper-->
<div class="d-flex flex-column flex-column-fluid">

    <!--begin::Content-->
    <div id="kt_app_content" class="app-content flex-column-fluid">
        <!--begin::Content container-->
        <div id="kt_app_content_container" class="app-container container-xxl" style="max-width:100% !important">
            <!--begin::Toolbar-->
            <div id="kt_app_toolbar" class="app-toolbar py-3 py-lg-6">
                <!--begin::Page title-->
                <div class="page-title d-flex flex-column justify-content-center flex-wrap me-3">
                    <!--begin::Title-->
                    <h1 class="page-heading d-flex text-dark fw-bold fs-3 flex-column justify-content-center my-0">@ViewBag.Title</h1>
                    <!--end::Title-->
                </div>
                <!--end::Page title-->
            </div>
            <!--end::Toolbar-->
            <!--begin::Card-->
            <div class="card mb-6">
                <!--begin::Card body-->
                <div class="card-body pt-0">

                    <div class="row align-items-center mt-7">
                        <div class="form-group col-md-3 mt-3">
                            <label>Rango de fechas</label>
                            <input class="form-control " placeholder="Pick date rage" id="rangoFechas" value="@(DateTime.Now.ToString("01/MM/yyyy")) - @(DateTime.Now.ToString("dd/MM/yyyy"))" />
                        </div>

                        <div class="form-group col-md-3 mt-3 ">
                            <label>Cuenta origen</label>
                            <select id="cuentabancaria" name="cuentabancaria" class="form-select" data-control="select2" data-placeholder="Filtrar por una cuenta bancaria">
                                <option></option>
                                <option selected value="0">Todas</option>
                                @{
                                    if (ViewBag.ListaCuentas != null)
                                    {
                                        foreach (var item in ViewBag.ListaCuentas)
                                        {
                                            <option value="@item.CUENTA_BANCO">@item.COMPLETO</option>
                                        }
                                    }
                                }

                            </select>
                        </div>
                        <div class="form-group col-md-3 mt-3 ">
                            <label>Tipo</label>
                            <select id="tipo" name="tipo" class="form-select" data-control="select2" data-placeholder="Filtrar por una tipo">
                                <option></option>
                                <option selected value="0">Todos</option>
                                <option value="TEF">TEF</option>
                                <option value="DEP">DEP</option>
                                <option value="T/C">T/C</option>
                                <option value="T/D">T/D</option>


                            </select>
                        </div>
                        <div class="form-group col-md-3 mt-3 ">
                            <label>Subtipo</label>
                            <select id="subtipo" name="subtipo" class="form-select" data-control="select2" data-placeholder="Filtrar por un subtipo">
                                <option></option>
                                <option selected value="00">Todos</option>
                                @{
                                    if (ViewBag.ListaSubtiposTODOS != null)
                                    {
                                        foreach (var item in ViewBag.ListaSubtiposTODOS)
                                        {
                                            <option value="@item.SUBTIPO">@item.SUBTIPO || @item.DESCRIPCION</option>
                                        }
                                    }
                                }


                            </select>
                        </div>
                        <div class="form-group col-md-3 mt-3 ">
                            <label>Documento</label>
                            <select id="docusoftland" name="docusoftland" class="form-select" data-control="select2" data-placeholder="Filtrar por un documento de Softland">
                                <option></option>
                                <option selected value="0">Todos</option>
                                @{
                                    if (ViewBag.ListaDocumentos != null)
                                    {
                                        foreach (var item in ViewBag.ListaDocumentos)
                                        {
                                            <option value="@item.DOCUMENTO">@item.DOCUMENTO</option>
                                        }
                                    }
                                }

                            </select>
                        </div>
                        @*<div class="form-group col-md-3 mt-3 ">
                                <label>Monto</label>
                                <select id="monto" name="monto" class="form-select" data-control="select2" data-placeholder="Filtrar por un monto">
                                    <option></option>
                                    <option selected value="0">Todos</option>

                                </select>
                            </div>*@
                        @*<div class="form-group col-md-3 mt-3 ">
                                <label>Estado CXC</label>
                                <select id="softlandcc" name="softlandcc" class="form-select" data-control="select2" data-placeholder="Filtrar por un estado cc">
                                    <option></option>
                                    <option selcted value="0">Todos</option>
                                    <option value="NO ERP CXC">NO ERP CXC</option>
                                    <option value="SI ERP CXC">SI ERP CXC</option>
                                    <option value="No aplica">No aplica</option>

                                </select>
                            </div>*@
                        @*<div class="form-group col-md-3 mt-3 ">
                                <label>Cargado ERP</label>
                                <select id="softlandcb" name="softlandcc" class="form-select" data-control="select2" data-placeholder="Filtrar por un estado cb">
                                    <option></option>
                                    <option selected value="0">Todos</option>
                                    <option  value="NO">NO</option>
                                    <option  value="SI ">SI</option>
                                    <option value="No aplica">No aplica</option>

                                </select>
                            </div>*@
                        <div class="form-group col-md-3 mt-3 ">
                            <label>Proveedor</label>
                            <select id="proveedor" name="proveedor" class="form-select" data-control="select2" data-placeholder="Filtrar por un proveedor">
                                <option></option>
                                <option selected value="0">Todos</option>
                                @{
                                    if (ViewBag.ListaProveedores != null)
                                    {
                                        foreach (var item in ViewBag.ListaProveedores)
                                        {
                                            <option value="@item.PROVEEDOR">@item.COMPLETO</option>
                                        }
                                    }
                                }

                            </select>
                        </div>
                        <div class="form-group col-md-3 mt-3 ">
                            <label>Cliente</label>
                            <select id="cliente" name="cliente" class="form-select" data-control="select2" data-placeholder="Filtrar por un cliente">
                                <option></option>
                                <option selected value="0">Todos</option>
                                @{
                                    if (ViewBag.ListaClientes != null)
                                    {
                                        foreach (var item in ViewBag.ListaClientes)
                                        {
                                            <option value="@item.CLIENTE">@item.COMPLETO</option>
                                        }
                                    }
                                }

                            </select>
                        </div>
                        <div class="form-group col-md-3 mt-3 ">
                            <label>Débitos | Créditos</label>
                            <select id="credeb" name="credeb" class="form-select" data-control="select2" data-placeholder="Filtrar por créditos y débitos">
                                <option></option>
                                <option selected value="0">Todos</option>
                                <option value="C">Créditos</option>
                                <option value="D">Déditos</option>

                            </select>
                        </div>
                        <div class="form-group col-md-3 mt-3 ">
                            <label>CC | CP | CB</label>
                            <select id="cargado" name="cargado" class="form-select" data-control="select2" data-placeholder="Filtrar por cargado">
                                <option></option>
                                <option selected value="0">Todos</option>
                                <option value="CXC">CARGADO CC</option>
                                <option value="CXP">CARGADO CP</option>
                                <option value="CB">CARGADO CB</option>
                                <option value="N/D">NO CARGADO</option>

                            </select>
                        </div>
                        <div class="form-group col-md-3 mt-3 ">
                            <label>Cargado a conciliación bancaria</label>
                            <select id="conciliados" name="conciliados" class="form-select" data-control="select2" data-placeholder="Filtrar por conciliados">
                                <option></option>
                                <option selected value="0">Todos</option>
                                <option value="SI">SI</option>
                                <option value="NO">NO</option>

                            </select>
                        </div>
                        <div class="col-md-3 mt-8">
                            <div class="input-icon">
                                <input type="text" class="form-control" placeholder="Buscar..." id="kt_datatable_search_query" data-kt-docs-table-filter="search" autocomplete="off" />
                                <span>
                                    <i class="flaticon2-search-1 text-muted"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <!--end::Card body-->
            </div>
            <!--end::Card-->
            <!--begin::Card-->
            <div class="card">
                <!--begin::Card body-->
                <div class="card-body pt-0">
                    <table class="table align-middle table-striped fs-6 gy-4 table-responsive" id="miTabla">
                        <!--begin::Table head-->
                        <thead>
                            <!--begin::Table row-->
                            <tr class="text-start text-gray-600 fw-bold fs-7 text-uppercase gs-0">

                                @*<th class="min-w-100px">Order ID</th>*@
                                <th class="w-10px pe-2">
                                    <div class="form-check form-check-sm form-check-custom form-check-solid me-3">
                                        <input class="form-check-input" type="checkbox" id="checkAll" data-kt-check="true" data-kt-check-target="#miTabla .form-check-input" value="1" />
                                    </div>
                                </th>
                                <th class="text-left min-w-80px"># Banco</th>
                                <th class="text-left min-w-150px">Cuenta origen</th>
                                <th class="text-left min-w-40px">H2H</th>
                                <th class="text-left min-w-80px"># SFTL</th>
                                <th class="text-left min-w-50px">Tipo</th>
                                <th class="text-left min-w-75px">Subtipo</th>
                                <th class="text-left min-w-80px">CC | CP | CB</th>
                                <th class="text-left min-w-20px">CONC</th>
                                <th class="text-left min-w-75px">Fecha</th>
                                <th class="text-left min-w-250px">Concepto</th>
                                <th class="text-left min-w-100px">Débito</th>
                                <th class="text-left min-w-100px">Crédito</th>
                                @*<th class="text-left min-w-125px">Estado CXC</th>*@
                                @*<th class="text-left min-w-125px">Cargado ERP</th>*@
                                <th class="text-left min-w-100px">Proveedor</th>
                                <th class="text-left min-w-100px">Cliente</th>
                                <th class="text-left min-w-75px">Fecha MT</th>
                                @*<th class="text-left min-w-100px">Subtipo</th>*@
                                <th class="text-left min-w-150px">Acciones</th>
                                @*<th class="text-end"></th>*@
                            </tr>
                            <!--end::Table row-->
                        </thead>
                        <!--end::Table head-->
                        <!--begin::Table body-->
                        <tbody class="fw-bold text-gray-600">
                        </tbody>
                        <!--end::Table body-->
                    </table>
                    <!--end::Table-->
                </div>
                <!--end::Card body-->
            </div>
            <!--end::Card-->
        </div>
        <!--end::Content container-->
    </div>
    <!--end::Content-->
</div>
<!--end::Content wrapper-->

<div class="modal fade" tabindex="-1" id="modalAsociarCliente">
    <div class="modal-dialog modal-dialog-centered ">
