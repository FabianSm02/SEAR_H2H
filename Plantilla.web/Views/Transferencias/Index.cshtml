                        </svg>
                    </span>
                </div>
                <!--end::Close-->
            </div>

            <div class="modal-body">
                <div class="row">

                    <div class="form-group mb-6">
                        <label>Tipo</label>
                        <select id="tipo_cxc" data-dropdown-parent="#modalAsociarCliente" name="tipo_cxc" class="form-select" data-control="select2" data-placeholder="Seleccione un tipo">
                            <option></option>
                            <option value="TEF">TEF</option>
                            <option value="DEP">DEP</option>


                        </select>
                    </div>
                    <div class="form-group mb-6">
                        <label>Subtipo</label>
                        <select id="subtipo_cxc" data-dropdown-parent="#modalAsociarCliente" name="tipo_cxc" class="form-select" data-control="select2" data-placeholder="Seleccione un subtipo">
                            <option></option>
                            @{
                                if (ViewBag.ListaSubtiposCC != null)
                                {
                                    foreach (var item in ViewBag.ListaSubtiposCC)
                                    {
                                        <option value="@item.SUBTIPO">@item.SUBTIPO || @item.DESCRIPCION</option>
                                    }
                                }
                            }

                        </select>
                    </div>
                    <div class="form-group mb-6">
                        <label>Cliente a asignar</label>
                        <select id="cliente_cxc" data-dropdown-parent="#modalAsociarCliente" name="cliente_cxc" class="form-select" data-control="select2" data-placeholder="Seleccione un cliente">
                            <option></option>
                            @{
                                if (ViewBag.ListaClientes != null)
                                {
                                    foreach (var item in ViewBag.ListaClientes)
                                    {
                                        <option value="@item.CLIENTE">@item.COMPLETO</option>
                                    }
                                }
                            }

                        </select>
                    </div>
                    <div class="form-group mb-6">
                        <label>Concepto</label>
                        <input type="text" class="form-control" id="concepto_cxc" value="" />
                    </div>
                    <div class="form-group mb-6">
                        <label>Cuenta origen</label>
                        <input disabled type="text" class="form-control" id="cuentabancaria_cxc" value="" maxlength="22" />
                    </div>
                    <div class="form-group mb-6">
                        <label>Fecha</label>
                        <input disabled type="text" class="form-control" id="fechadoc_cxc" value="" />
                    </div>
                    <div class="form-group mb-6">
                        <label>Moneda</label>
                        <input disabled type="text" class="form-control" id="moneda_cxc" value="" />
                    </div>
                    <div class="form-group mb-6">
                        <label>Monto</label>
                        <input disabled type="number" class="form-control" id="monto_cxc" value="" min="0" />
                    </div>
                    <div class="form-group mb-6 d-none">
                        <label>Documento</label>
                        <input type="text" class="form-control" id="documento_cxc" value="" />
                    </div>
                    <div class="form-group mb-6 d-none">
                        <label>ID</label>
                        <input type="text" class="form-control" id="idlinea_cxc" value="" />
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-light" data-bs-dismiss="modal">Cancelar</button>
                <button type="button" onclick="GuardarAsignarCC()" class="btn btn-success">Guardar</button>
            </div>
        </div>
    </div>
</div>
